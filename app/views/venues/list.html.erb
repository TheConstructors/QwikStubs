<!-- nav bar for venue -->
<ul class="nav nav-tabs">
  <li class="active"> <%= link_to "Home", "/venue" %> </li>
  <li> <%= link_to "Create", "/venue/create" %> </li>
</ul>

<!-- end nav bar for venue -->

<% if @venues.blank? %>
<h1>There are no venues</h1>


<% else %>

<table class="table">
<tr>
  <th>Name</th>
  <th>City</th>
  <th>State</th>
</tr>
<% @venues.each do |v| %>
<tr>
  <td><%= link_to v.name, "/venue/show/" + v.name %> </td>
  <td><%= v.city %> </td>
  <td><%= v.state %> </td>
</tr>
<% end %>
</table>
<% end %>
